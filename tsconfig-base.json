{
    "compilerOptions": {
        "pretty": true,
        "lib": ["es2015.iterable", "es2015.generator", "es5", "dom"],
        "target": "es5",
        "moduleResolution": "node",
        "rootDir": ".",

        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,

        "skipLibCheck": true,
        "strict": true,
        "preserveConstEnums": true,
        "incremental": true,

        "types": []
    }
}
